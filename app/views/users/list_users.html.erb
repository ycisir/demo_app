<%= render 'common_templates/company_name' %>

<h1>List Users</h1>

<table border="1">
	<th>First name</th>
	<th>Last name</th>
	<th>Age</th>
	<th>City</th>
	<th>Gender</th>

	<% @users.each do |user| %>
		<tr>
			<td><%= user.first_name %></td>
			<td><%= user.last_name %></td>
			<td><%= user.age %></td>
			<td><%= user.city %></td>
			<td><%= user.gender_string %></td> <%# for smaller function use in model else helper>
		</tr>
	<% end %>
</table>
